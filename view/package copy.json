    "@vue/compiler-sfc": "3.1.1",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/test-utils": "^2.0.0-rc.14",
    "babel-eslint": "10.1.0",
    "electron": "12.0.0",
    "electron-devtools-installer": "3.1.0",
    "eslint": "6.7.2",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-vue": "7.0.0",
    "jest-html-reporter": "^3.4.1",
    "node-sass": "4.14.1",
    "prettier": "2.2.1",
