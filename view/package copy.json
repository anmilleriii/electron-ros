{
  "name": "remi",
  "version": "0.1.0",
  "private": true,
  "description": "REMote Interface (REMI) for M220 autonomous mowers.",
  "author": "FireFly Automatix, Inc.",
  "scripts": {
    "serve": "vue-cli-service serve ./src/renderers/view/main.js",
    "build": "vue-cli-service build ./src/renderers/view/main.js",
    "test:unit": "vue-cli-service test:unit --testPathIgnorePatterns=./test/e2e/",
    "test:unit:watch": "vue-cli-service test:unit --watch",
    "test:e2e": "vue-cli-service test:unit --testMatch=**/test/e2e/*.spec.js",
    "lint": "vue-cli-service lint",
    "electron:build:linux": "vue-cli-service electron:build --linux --x64",
    "electron:build:mac": "vue-cli-service electron:build --mac",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "release:linux": "npm run electron:build:linux -- -p always",
    "release:mac": "npm run electron:build:mac -- -p always",
    "serve:production": "vue-cli-service serve --mode production",
    "docs": "cd ../docs && npm run dev && cd ../app",
    "server": "json-server ./data/db.json  --watch"
  },
  "main": "background.js",
  "dependencies": {
    "@auth0/auth0-spa-js": "1.15.0",
    "@turf/turf": "6.5.0",
    "@types/jest": "^24.9.1",
    "axios": "0.21.1",
    "browser-battery": "2.0.0",
    "bufferutil": "4.0.3",
    "core-js": "3.6.5",
    "electron-debug": "3.2.0",
    "electron-is-dev": "2.0.0",
    "electron-log": "4.4.0",
    "electron-updater": "4.3.9",
    "express": "4.17.1",
    "ffmpeg-static": "4.4.0",
    "json-server": "0.16.3",
    "leaflet": "1.7.1",
    "lodash": "4.17.21",
    "node-rtsp-stream": "0.0.9",
    "socket.io": "4.1.3",
    "socket.io-client": "4.1.3",
    "typeface-roboto": "1.1.13",
    "utf-8-validate": "5.0.5",
    "uuid": "8.3.2",
    "vue": "3.1.1",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.0",
